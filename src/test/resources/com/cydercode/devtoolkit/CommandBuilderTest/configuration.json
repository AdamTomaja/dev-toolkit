{
  "applications": {
    "Maven": {
      "path": "mvn"
    }
  },
  "parameters": {
    "distribution": {
      "type": "string",
      "values": [
        "A",
        "B",
        "C"
      ]
    },
    "profile": {
      "type": "string",
      "values": [
        "local",
        "dev",
        "test"
      ]
    }
  },
  "presets": {
    "build": {
      "name": "build",
      "application": "Maven",
      "cmd": "clean install -d ${distribution} -p ${profile}"
    }
  }
}